{
  "name": "OTP Debug Tiles",
  "sources": {
    "background": {
      "id": "background",
      "tiles": ["https://a.tile.openstreetmap.org/{z}/{x}/{y}.png"],
      "maxzoom": 19,
      "tileSize": 256,
      "attribution": "Â© OpenStreetMap Contributors",
      "type": "raster"
    },
    "vectorSource": {
      "id": "vectorSource",
      "url": "https://example.com",
      "type": "vector"
    }
  },
  "layers": [
    {
      "id": "background",
      "type": "raster",
      "source": "background",
      "minzoom": 0,
      "metadata": {
        "group": "Other"
      }
    },
    {
      "id": "wheelchair-accessible",
      "source": "vectorSource",
      "source-layer": "edges",
      "type": "line",
      "minzoom": 6,
      "maxzoom": 23,
      "paint": {
        "line-color": "#136b04",
        "line-width": ["interpolate", ["linear"], ["zoom"], 13, 0.2, 23, 8.0],
        "line-offset": ["interpolate", ["linear"], ["zoom"], 13, 0.4, 23, 7.0]
      },
      "filter": ["==", "wheelchairAccessible", true],
      "layout": {
        "line-cap": "round",
        "visibility": "none"
      },
      "metadata": {
        "group": "Wheelchair accessibility"
      }
    },
    {
      "id": "wheelchair-inaccessible",
